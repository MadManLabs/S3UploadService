{
  "AppSettings": {
    "LookFolder": "c:\\folder\\for_upload",
    "DoneFolder": "c:\\folder\\done",
    "FailedFolder": "c:\\folder\\failed",
    "MaxRetries": 5,
    "S3AccessKey": "access_key",
    "S3SecretKey": "secret_key",
    "Region": "eu-central-1",
    "Bucket": "upload_bucket",
    "StartKey": "uploaded",
    "WakeupSeconds": 30,
    "GraceSeconds": 10,
    "FileMask": "*.*"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
